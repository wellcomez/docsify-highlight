{
  "name": "docsify-highlight",
  "version": "2.2.14",
  "jsdelivr": [
    "dist/docsify-highlight.css",
    "dist/docsify-highlight.umd.min.js",
    "dist/docsify-highlight.umd.js.map",
    "dist/fonts",
    "dist/img"
  ],
  "main": "dist/docsify-highlight.umd.min.js",
  "browser": "docsify-highlight.css",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "rel": "npm run lib && npm publish",
    "lib": "vue-cli-service build --target lib  --name docsify-highlight  src/entry.js",
    "build": "vue-cli-service build --target lib  --name docsify-highlight  src/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wellcomez/docsify-highlight.git"
  },
  "keywords": [
    "docsify",
    "highlight"
  ],
  "files": [
    "src",
    ".babelrc",
    "babel.config.js",
    "vue.config.js",
    "dist/docsify-highlight.css",
    "dist/docsify-highlight.umd.min.js",
    "dist/fonts",
    "dist/img"
  ],
  "dependencies": {
    "@toast-ui/vue-editor": "^3.0.2",
    "clipboard-copy": "^4.0.1",
    "color-alpha": "^1.1.2",
    "color-rgba": "^2.2.3",
    "colr": "^1.2.2",
    "core-js": "^3.6.5",
    "file-saver": "^2.0.5",
    "is-mobile": "^3.0.0",
    "iview": "^3.5.4",
    "js-base64": "^3.6.0",
    "leancloud-storage": "^4.10.0",
    "md5": "^2.3.0",
    "medium-zoom": "^1.0.6",
    "values.js": "^2.1.1",
    "vue": "^2.6.11",
    "vue-click-outside": "^1.1.0",
    "vue-lazyload": "^1.3.3",
    "vue-scrollto": "^2.20.0",
    "vue2-touch-events": "^3.2.1",
    "web-highlighter": "^0.7.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.2",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
